{
  "exclude": ["./src/server"],
  "presets": [
    "mobx",
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ],
  "overrides": [{
    "include": ["./src/server", "./src/utils"],
    "presets": [
      ["@babel/preset-env", {"targets": {"node": "current"}}]
    ]
  }]
}