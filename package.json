{
  "name": "webpack-ui",
  "productName": "webpack UI",
  "version": "0.0.1",
  "description": "webpack UI",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config .webpack/webpack.config.js",
    "build": "rimraf dist && webpack watch --config .webpack/webpack.config.js --progress --color --profile --json > webpack-stats.json",
    "server": "nodemon src/server/server.js",
    "ui": "concurrently --kill-others \"npm run build\" \"npm run start\" \"npm run server\""
  },
  "author": "Even Stensberg",
  "license": "MIT",
